includes:
  # - phpstan-baseline.neon

parameters:
  level: 5
  tmpDir: build/phpstan
  reportUnmatchedIgnoredErrors: false

  paths:
    - src
    # - tests

  ignoreErrors:
    - '#^Unsafe usage of new static\(\).$#'
    - '#^Parameter \#1 \$separator of function explode expects non\-empty\-string, string given\.$#'
