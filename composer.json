{
  "name": "webtheory/saveyour",
  "description": "Modular form building and processing library",
  "homepage": "https://github.com/spider-mane/saveyour",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com",
      "homepage": "https://github.com/spider-mane",
      "role": "Developer"
    }
  ],
  "keywords": [
    "webtheory",
    "saveyour"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4 || ^8.0",
    "myclabs/php-enum": "^1.7",
    "psr/http-message": "^1.0",
    "respect/validation": "^2.2",
    "webtheory/factoryfactory": "@dev",
    "webtheory/html": "@stable"
  },
  "require-dev": {
    "commerceguys/addressing": "^1.0",
    "captainhook/captainhook": "^5.10",
    "egulias/email-validator": "^3.1",
    "fakerphp/faker": "^1.16",
    "friendsofphp/php-cs-fixer": "3.4",
    "geocoder-php/google-maps-provider": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.12",
    "guzzlehttp/guzzle": "^7.4",
    "guzzlehttp/psr7": "^2.1",
    "hubspot/hubspot-php": "^3.2",
    "league/fractal": "^0.19",
    "mockery/mockery": "^1.2",
    "moneyphp/money": "^3.2",
    "nesbot/carbon": "^2.46",
    "oscarotero/env": "^2.1",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/message": "^1.8",
    "phpbench/phpbench": "@dev",
    "phpstan/phpstan": "^1.3",
    "phpunit/phpunit": "^9.5",
    "phpunit/phpunit-selenium": "^9.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psy/psysh": "^0.11",
    "spatie/phpunit-watcher": "^1.12",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/mailer": "^5.4",
    "symfony/thanks": "^1.1",
    "symfony/validator": "^5.4",
    "vimeo/psalm": "^4.7",
    "vlucas/phpdotenv": "^5.3",
    "webtheory/exterminate": "@stable"
  },
  "autoload": {
    "psr-4": {
      "WebTheory\\Saveyour\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "suggest": {
    "frontpack/composer-assets-plugin": "Copy assets from vendor directory to pubic web folder",
    "symfony/mailer": "Send email based on form submission values",
    "commerceguys/addressing": "Required to use FormAddressGeocoder form processor",
    "giggsey/libphonenumber-for-php": "Required to use PhoneNumberDataFormatter"
  },
  "scripts": {
    "asset": "npm --prefix assets",
    "bench": "vendor/bin/phpbench",
    "check": "vendor/bin/phpcs",
    "collect": "vendor/bin/var-dump-server",
    "env": "cp -n .env.example .env && bin/build-paths",
    "fix": "vendor/bin/php-cs-fixer fix --verbose",
    "hooks": "vendor/bin/captainhook",
    "inspect": "vendor/bin/psalm",
    "shell": "vendor/bin/psysh",
    "test": "vendor/bin/phpunit",
    "test:acceptance": "@test --testsuite acceptance",
    "test:functional": "@test --testsuite functional",
    "test:integration": "@test --testsuite integration",
    "test:unit": "@test --testsuite unit",
    "watch": "vendor/bin/phpunit-watcher",
    "build": [
      "@fix",
      "@check",
      "@inspect",
      "@test"
    ],
    "post-install-cmd": [
      "@env"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "process-timeout": 0,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/thanks": true
    }
  },
  "extra": {
    "assets-files": [
      "assets/dist/saveyour.js"
    ],
    "build-paths": {
      "build": {
        "phpunit": {},
        "php-cs-fixer": {}
      },
      "logs": {},
      "temp": {}
    }
  }
}
